{
    "type": "array",
    "properties": [
        {
            "key": "cluster",
            "description": "cluster properties",
            "type": "array",
            "properties": [
                {
                    "key": "name",
                    "type": "string",
                    "label": "Name",
                    "description": "The name of the application",
                    "default": "MongoDB Sharded Cluster",
                    "required": "no"
                },
                {
                    "key": "description",
                    "type": "string",
                    "label": "Description",
                    "description": "The description of the application",
                    "default": "",
                    "required": "no"
                },
                {
                    "key": "vxnet",
                    "label": "VxNet",
                    "description": "The vxnet that the application will join",
                    "type": "string",
                    "default": "",
                    "required": "yes"
                },{
                    "key": "resource_group",
                    "label": "Express Configuration",
                    "description": "Test: Config Server * 3, Shard * 1, Mongos * 1 </br>Prod: Config Server * 3, Shard * 2, Mongos * 2 </br>Note: each Shard consists of 1 primary node + 2 replica nodes",
                    "type": "string",
                    "default": "Test",
                    "range": [
                          "Test",
                          "Prod"
                      ]
                },{
                    "key": "cs_node",
                    "description": "Properties for config server node",
                    "label": "Config Server Node",
                    "type": "array",
                    "properties": [
                        {
                            "key": "instance_class",
                            "label": "Instance Class",
                            "description": "The instance type for the config server node",
                            "type": "integer",
                            "default": 101,
                            "range": [101],
                            "required": "yes",
                            "resource_group": [101, 101]
                        },
                        {
                            "key": "cpu",
                            "label": "CPU",
                            "description": "CPUs of each node",
                            "type": "integer",
                            "range": [1],
                            "default": 1,
                            "required": "yes",
                            "resource_group": [1, 1]
                        },
                        {
                            "key": "memory",
                            "description": "Memory of each node",
                            "label": "Memory",
                            "type": "integer",
                            "range": [
                                2048
                              ],
                            "default": 2048,
                            "required": "yes",
                            "resource_group": [2048, 2048]
                        },
                        {
                            "key": "volume_class",
                            "label": "Volume Class",
                            "description": "The volume type for each node",
                            "type": "integer",
                            "range": [100],
                            "default": 100,
                            "required": "yes",
                            "resource_group": [100, 100]
                        },
                        {
                            "key": "volume_size",
                            "label": "Volume Size",
                            "description": "The volume size for each instance",
                            "type": "integer",
                            "default": 20,
                            "min": 20,
                            "step": 20,
                            "required": "yes"
                        },
                        {
                            "key": "count",
                            "label": "Node Count",
                            "description": "Each ConfigServer defaults to a replica set architecture (three nodes), and modification is not currently supported.",
                            "type": "integer",
                            "min": 3,
                            "max": 3,
                            "default": 3,
                            "range": [3],
                            "readonly": "yes",
                            "required": "yes"
                        }
                    ]
                },
                {
                    "key": "shard_node",
                    "description": "Properties for shard node",
                    "label": "Sharded Node",
                    "type": "array",
                    "properties": [
                        {
                            "key": "instance_class",
                            "label": "Instance Class",
                            "description": "The instance type for the shard node",
                            "type": "integer",
                            "default": 202,
                            "range": [101, 202],
                            "required": "yes",
                            "resource_group": [101, 202]
                        },
                        {
                            "key": "cpu",
                            "label": "CPU",
                            "description": "CPUs of each node",
                            "type": "integer",
                            "range": [1, 2, 4, 8, 16, 32, 64],
                            "default": 2,
                            "required": "yes",
                            "resource_group": [1, 2]
                        },
                        {
                            "key": "memory",
                            "description": "Memory of each node",
                            "label": "Memory",
                            "type": "integer",
                            "range": [
                                1024,
                                2048,
                                4096,
                                8192,
                                16384,
                                32768,
                                65536,
                                131072,
                                262144
                              ],
                            "default": 8192,
                            "required": "yes",
                            "resource_group": [2048, 8192]
                        },
                        {
                            "key": "volume_class",
                            "label": "Volume Class",
                            "description": "The volume type for each node",
                            "type": "integer",
                            "range": [2, 5, 100, 200],
                            "default": 200,
                            "required": "yes",
                            "resource_group": [100, 200]
                        },
                        {
                            "key": "volume_size",
                            "label": "Volume Size",
                            "description": "The volume size for each instance",
                            "type": "integer",
                            "default": 200,
                            "min": 200,
                            "step": 50,
                            "required": "yes"
                        },
                        {
                            "key": "count",
                            "label": "Node Count",
                            "description": "Number of Shard nodes to be created, can NOT be decreased if the cluster has been created, each Shard consists of 1 primary node + 2 replica nodes",
                            "type": "integer",
                            "min": 1,
                            "max": 16,
                            "default": 2,
                            "required": "yes",
                            "resource_group": [1, 2]
                        }
                    ]
                },
                {
                    "key": "mongos_node",
                    "description": "Properties for mongos node",
                    "label": "Mongos Node",
                    "type": "array",
                    "properties": [
                        {
                            "key": "instance_class",
                            "label": "Instance Class",
                            "description": "The instance type for the Mongos node",
                            "type": "integer",
                            "default": 202,
                            "range": [101, 202],
                            "required": "yes",
                            "resource_group": [101, 202]
                        },
                        {
                            "key": "cpu",
                            "label": "CPU",
                            "description": "CPUs of each node",
                            "type": "integer",
                            "range": [1, 2, 4, 8, 16, 32, 64],
                            "default": 2,
                            "required": "yes",
                            "resource_group": [1, 2]
                        },
                        {
                            "key": "memory",
                            "description": "Memory of each node",
                            "label": "Memory",
                            "type": "integer",
                            "range": [
                                1024,
                                2048,
                                4096,
                                8192,
                                16384,
                                32768,
                                65536,
                                131072,
                                262144
                              ],
                            "default": 8192,
                            "required": "yes",
                            "resource_group": [2048, 8192]
                        },
                        {
                            "key": "volume_class",
                            "label": "Volume Class",
                            "description": "The volume type for each node",
                            "type": "integer",
                            "range": [2, 5, 100, 200],
                            "default": 200,
                            "required": "yes",
                            "resource_group": [100, 200]
                        },
                        {
                            "key": "volume_size",
                            "label": "Volume Size",
                            "description": "The volume size for each instance",
                            "type": "integer",
                            "default": 10,
                            "min": 10,
                            "step": 10,
                            "required": "yes"
                        },
                        {
                            "key": "count",
                            "label": "Node Count",
                            "description": "Each Mongos defaults to a single-node architecture, and you need to purchase more than two to ensure high availability.",
                            "type": "integer",
                            "min": 1,
                            "max": 10,
                            "default": 2,
                            "required": "yes",
                            "resource_group": [1, 2]
                        }
                    ]
                }
            ]
        },
        {
            "key": "env",
            "description": "Application configuration properties",
            "type": "array",
            "properties": [
                {
                    "key": "conf.userPass",
                    "label": "User root's Password",
                    "description": "Must contain a combination of uppercase and lowercase letters and numbers, special characters can be used (including @#%&*()_+-=), and the length is between 8-32",
                    "type": "password",
                    "pattern": "^(?=.*\\d)(?=.*[a-z])(?=.*[A-Z])[0-9a-zA-Z@#\\%\\&\\*\\(\\)\\_\\+\\-\\=]{8,32}$",
                    "default": "Change1Pwd",
                    "required": "yes"
                },
                {
                    "key": "conf.storage.engine",
                    "label": "Storage engine",
                    "description": "The storage engine used by MongoDB, can NOT be changed",
                    "type": "string",
                    "default": "wiredTiger",
                    "range": ["wiredTiger"],
                    "changeable": false,
                    "required": "no"
                },
                {
                    "key": "mongos_node",
                    "description": "The role configuration properties of the application",
                    "type": "array",
                    "properties": [{
                        "key": "conf.net.port",
                        "label": "Mongos' net.port",
                        "description": "Can NOT be set to 27099, need restart mongos.service if being changed, default value: 27018",
                        "type": "integer",
                        "default": 27018,
                        "pattern": "^(?!27099)(102[4-9]|10[3-9]\\d|1[1-9]\\d\\d|[2-9]\\d{3}|[1-5]\\d{4}|6[0-4]\\d{3}|65[0-4]\\d{2}|655[0-2]\\d|6553[0-5])$",
                        "required": "no",
                        "min": 1025,
                        "max": 65535
                    }, {
                        "key": "conf.setParameter.cursorTimeoutMillis",
                        "label": "Mongos' setParameter.cursorTimeoutMillis",
                        "description": "Unit: ms, default value: 600000",
                        "type": "integer",
                        "default": 600000,
                        "required": "no"
                    }]
                },
                {
                    "key": "cs_node",
                    "description": "The role configuration properties of the application",
                    "type": "array",
                    "properties": [{
                        "key": "conf.net.port",
                        "label": "Config Server's net.port",
                        "description": "Can NOT be set to 27099, can NOT be changed if the cluster has been created, default value: 27019",
                        "type": "integer",
                        "default": 27019,
                        "pattern": "^(?!27099)(102[4-9]|10[3-9]\\d|1[1-9]\\d\\d|[2-9]\\d{3}|[1-5]\\d{4}|6[0-4]\\d{3}|65[0-4]\\d{2}|655[0-2]\\d|6553[0-5])$",
                        "changeable": false,
                        "required": "no",
                        "min": 1025,
                        "max": 65535
                    }, {
                        "key": "conf.setParameter.cursorTimeoutMillis",
                        "label": "Config Server's setParameter.cursorTimeoutMillis",
                        "description": "Unit: ms, default value: 600000",
                        "type": "integer",
                        "default": 600000,
                        "required": "no"
                    }, {
                        "key": "conf.operationProfiling.mode",
                        "label": "Config Server's operationProfiling.mode",
                        "description": "[off, slowOp, all], default value: off",
                        "type": "string",
                        "range": ["off", "slowOp", "all"],
                        "default": "off",
                        "required": "no"
                    }, {
                        "key": "conf.operationProfiling.slowOpThresholdMs",
                        "label": "Config Server's operationProfiling.slowOpThresholdMs",
                        "description": "Unit: ms, default value: 100",
                        "type": "integer",
                        "default": 100,
                        "required": "no"
                    }, {
                        "key": "conf.replication.oplogSizeMB",
                        "label": "Config Server's replication.oplogSizeMB",
                        "description": "Unit: MB, defalut value: 2048",
                        "type": "integer",
                        "default": 2048,
                        "required": "no",
                        "min": 990,
                        "max": 51200
                    }]
                },
                {
                    "key": "shard_node",
                    "description": "The role configuration properties of the application",
                    "type": "array",
                    "properties": [{
                        "key": "conf.net.port",
                        "label": "Shard's net.port",
                        "description": "Can NOT be set to 27099, can NOT be changed if the cluster has been created, default value: 27018",
                        "type": "integer",
                        "default": 27018,
                        "pattern": "^(?!27099)(102[4-9]|10[3-9]\\d|1[1-9]\\d\\d|[2-9]\\d{3}|[1-5]\\d{4}|6[0-4]\\d{3}|65[0-4]\\d{2}|655[0-2]\\d|6553[0-5])$",
                        "changeable": false,
                        "required": "no",
                        "min": 1025,
                        "max": 65535
                    }, {
                        "key": "conf.setParameter.cursorTimeoutMillis",
                        "label": "Shard's setParameter.cursorTimeoutMillis",
                        "description": "Unit: ms, default value: 600000",
                        "type": "integer",
                        "default": 600000,
                        "required": "no"
                    }, {
                        "key": "conf.operationProfiling.mode",
                        "label": "Shard's operationProfiling.mode",
                        "description": "[off, slowOp, all], default value: off",
                        "type": "string",
                        "range": ["off", "slowOp", "all"],
                        "default": "off",
                        "required": "no"
                    }, {
                        "key": "conf.operationProfiling.slowOpThresholdMs",
                        "label": "Shard's operationProfiling.slowOpThresholdMs",
                        "description": "Unit: ms, default value: 100",
                        "type": "integer",
                        "default": 100,
                        "required": "no"
                    }, {
                        "key": "conf.replication.enableMajorityReadConcern",
                        "label": "Shard's replication.enableMajorityReadConcern",
                        "description": "Need restart mongod.service if being changed, default value: true",
                        "type": "string",
                        "default": "true",
                        "range": ["true", "false"],
                        "required": "no"
                    }, {
                        "key": "conf.replication.oplogSizeMB",
                        "label": "Shard's replication.oplogSizeMB",
                        "description": "Unit: MB, defalut value: 20480",
                        "type": "integer",
                        "default": 20480,
                        "required": "no",
                        "min": 990,
                        "max": 51200
                    }]
                }
            ]
        }
    ]
}